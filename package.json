{
  "name": "vue-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "PORT=8000 node server",
    "server:dev": "PORT=8888 node --inspect server",
    "start": "NODE_ENV=production node server",
    "build:client": "NODE_ENV=production TYPE=PWA webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:ssr": "rimraf dist && npm run build:client && npm run build:server",
    "client:dev": "NODE_ENV=development webpack-dev-server --hot --config  build/webpack.client.dev.js",
    "build:spa": "rimraf dist_spa && NODE_ENV=production TYPE=SPA webpack --config build/webpack.client.config.js --progress --hide-modules",
    "test:client": "http-server ./dist_spa -p 8889",
    "mock": "node mock",
    "analyz": "NODE_ENV=production npm_config_report=true npm run build:client"
  },
